  "display": "standalone",
  "display_override": ["window-controls-overlay", "minimal-ui"],
  "background_color": "#ffffff",
  "theme_color": "#0d6efd",
  "orientation": "portrait",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "shortcuts": [
    {
      "name": "Add Transaction",
      "short_name": "Add Tx",
      "description": "Quickly add income or expense",
      "url": "/Wealth-Command/#add-transaction",
      "icons": [
        {
          "src": "/Wealth-Command/icons/add-192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "View Dashboard",
      "short_name": "Dashboard",
      "description": "See financial overview",
      "url": "/Wealth-Command/#dashboard",
      "icons": [
        {
          "src": "/Wealth-Command/icons/dashboard-192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Transaction History",
      "short_name": "History",
      "description": "Browse all transactions",
      "url": "/Wealth-Command/#transactions",
      "icons": [
        {
          "src": "/Wealth-Command/icons/history-192.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Financial Planner",
      "short_name": "Planner",
      "description": "Plan your future finances",
