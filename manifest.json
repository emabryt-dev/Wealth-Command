{
  "id": "/Wealth-Command/",
  "name": "Wealth Command Pro",
  "short_name": "WealthCommand",
  "description": "Expense and Income Manager - Track your finances effortlessly",
  "categories": ["finance", "productivity", "business"],
  "lang": "en",
